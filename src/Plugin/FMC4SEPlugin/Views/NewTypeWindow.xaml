<Window x:Class="MDD4All.FMC4SE.Plugin.Views.NewTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDD4All.FMC4SE.Plugin.Views"
        mc:Ignorable="d"
        Title="New Type" Height="150" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False">
	<Grid FocusManager.FocusedElement="{Binding ElementName=textBox}">
		<TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="21,37,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top"/>
		
		<TextBox x:Name="textBox" HorizontalAlignment="Left" Height="22"
				 Margin="70,34,0,0" TextWrapping="Wrap" Text="{Binding Name, Mode=TwoWay}" VerticalAlignment="Top" Width="212" />
		
		<Button x:Name="okButton" Content="Ok" HorizontalAlignment="Left" Margin="21,85,0,0" VerticalAlignment="Top" Width="75" Click="okButton_Click" IsDefault="True"/>
		
		<Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="123,85,0,0" VerticalAlignment="Top" Width="75" Click="cancelButton_Click" IsCancel="True"/>

	</Grid>
</Window>
